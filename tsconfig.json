{
    "compilerOptions": {
        "noImplicitAny": false,
        "lib": [
            "es2015"
        ],
        "target": "ES6",
        "module": "commonjs",
        "outDir": "./dist",
        "rootDir": "./src",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "sourceMap": true
    },
    "include": ["src"],
    "exclude": ["node_modules", "dist"]
}